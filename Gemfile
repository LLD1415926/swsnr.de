  gem "jekyll-github-metadata", git: "https://github.com/jekyll/github-metadata.git"
  gem "jekyll-redirect-from"
  gem "jekyll-feed"
  gem "jekyll-sitemap"
  gem "jekyll-default-layout"
  gem "jekyll-relative-links", git: "https://github.com/benbalter/jekyll-relative-links.git"
end

gem "webrick", "~> 1.7"
